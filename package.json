{
  "name": "the-custodians-archive",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "A multiplayer dashboard application for Twilight Imperium 4 that enables seamless game state tracking across multiple devices with real-time synchronization.",
  "scripts": {
    "dev": "doppler run -- vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\"",
    "debug-db-url": "doppler run -- node scripts/debug-connection-string.js",
    "test-db": "doppler run -- node scripts/test-db-connection.js",
    "fetch-rls": "doppler run -- node scripts/fetch-rls-node.js",
    "fetch-rls:save": "doppler run -- node scripts/fetch-rls-node.js --output docs/rls-policies.md"
  },
  "keywords": [
    "twilight-imperium",
    "ti4",
    "dashboard",
    "multiplayer",
    "game-tracker"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.83.0",
    "@vitejs/plugin-react": "^5.1.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "vite": "^7.2.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "pg": "^8.16.3",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.47.0"
  }
}
